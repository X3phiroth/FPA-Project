                </Menu>
                <Menu mnemonicParsing="false" text="Filter">
                    <items>
                        <MenuItem fx:id="menu_Filter" mnemonicParsing="false" text="Set Filter" />
                    </items>
                </Menu>
                <Menu mnemonicParsing="false" text="Help">
                    <items>
                        <MenuItem fx:id="menu_About" mnemonicParsing="false" text="About" />
